                    }
                ]
            }
        },
        "cooldown": 40,
        "key": {
            "key": "key.jump"
        }
    },
    "mount": {
        "type": "origins:action_on_entity_use",
        "bientity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:mount"
                },
                {
                    "type": "origins:target_action",
                    "action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "tag @s add mounted.z"
                            }
                        ]
                    }
                }
            ]
        },
        "bientity_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:distance",
                    "comparison": "<=",
                    "compare_to": 5
                },
                {
                    "type": "origins:target_condition",
                    "condition": {
                        "inverted": true,
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:entity_type",
                                "entity_type": "minecraft:horse"
                            },
                            {
                                "type": "origins:entity_type",
                                "entity_type": "minecraft:zombie_horse"
                            },
                            {
                                "type": "origins:entity_type",
                                "entity_type": "minecraft:skeleton_horse"
                            },
                            {
                                "type": "origins:entity_type",
                                "entity_type": "minecraft:pig"
                            },
                            {
                                "type": "origins:entity_type",
                                "entity_type": "minecraft:mule"
                            },
                            {
                                "type": "origins:entity_type",
                                "entity_type": "minecraft:donkey"
                            },
                            {
                                "type": "origins:entity_type",
                                "entity_type": "minecraft:strider"
                            },
                            {
                                "type": "origins:entity_type",
                                "entity_type": "minecraft:horse"
                            }
                        ]
                    }
                }
            ]
        },
        "item_condition": {
            "type": "origins:empty"
        },
        "hands": [
            "main_hand"
        ],
        "action_result": "success",
        "priority": 0
    },
    "reset": {
        "condition": {
            "inverted": true,
            "type": "origins:riding"
        },
        "type": "origins:action_over_time",
        "interval": 20,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "tag @e[tag=mounted.z,limit=1,sort=nearest] remove mounted.z"
                }
            ]
        }
    }
