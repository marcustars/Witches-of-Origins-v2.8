{
    "name": "Animal Empress",
    "description": "All the animals now praise you, their Empress, the one who tamed them all.\n\nUse right-click to mount ANY entity and the movement keys to control them.\n",
    "type": "origins:multiple",
    "forward": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:riding_action",
            "action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:execute_command",
                        "command": "data modify entity @s Rotation[0] set from entity @p Rotation[0]"
                    },
                    {
                        "type": "origins:execute_command",
                        "command": "data modify entity @s Motion[0] set from entity @p Motion[0]"
                    },
                    {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:or",
                            "conditions": [
                                {
                                    "type": "origins:on_block"
                                },
                                {
                                    "type": "origins:fluid_height",
                                    "fluid": "minecraft:water",
                                    "comparison": ">=",
                                    "compare_to": 0
                                },
                                {
                                    "type": "origins:fluid_height",
                                    "fluid": "minecraft:lava",
                                    "comparison": ">=",
                                    "compare_to": 0
                                }
                            ]
                        },
                        "if_action": {
                            "type": "origins:add_velocity",
                            "z": 0.075,
                            "space": "local_horizontal_normalized",
                            "client": true,
                            "server": true
                        },
                        "else_action": {
                            "type": "origins:add_velocity",
                            "z": 0.015,
                            "space": "local_horizontal_normalized",
                            "client": true,
                            "server": true
                        }
                    }
                ]
            }
        },
        "key": {
            "key": "key.forward",
            "continuous": true
        }
    },
